<img src=" /assets/ParkNarkCenter.png "alt="Smiley face"  height="81px" width="82%" onmouseover="this.src=' /assets/ParkNarkRight.gif '" onmouseout="this.src=' /assets/ParkNarkCenter.png '"class="img-responsive center-block">
<h2>Forgot your password?</h2>

<%= form_for(resource, as: resource_name, url: password_path(resource_name), html: { method: :post }) do |f| %>
  <%= devise_error_messages! %>

  <div class="field" id="email-fyp">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true %>
  </div>

  <div class="actions">
    <%= f.submit "Send" %>
  </div>
<% end %>

<%#= render "devise/shared/links" %>

<%- if controller_name != 'sessions' %>
  <div class="li-su">
    <%= link_to "LOG IN", new_session_path(resource_name) %><br />
  </div>
<% end -%>